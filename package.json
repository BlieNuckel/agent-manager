{
  "name": "agent-manager",
  "version": "2.0.0",
  "description": "TUI for managing multiple Claude Agent SDK instances",
  "type": "module",
  "scripts": {
    "start": "node --import tsx src/index.tsx",
    "dev": "node --import tsx --watch src/index.tsx",
    "demo": "node --import tsx src/demo/index.tsx",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@anthropic-ai/claude-agent-sdk": "^0.1.0",
    "@anthropic-ai/sdk": "^0.71.0",
    "@crosscopy/clipboard": "^0.2.8",
    "@iarna/toml": "^2.2.5",
    "gray-matter": "^4.0.3",
    "handlebars": "^4.7.8",
    "ink": "^5.1.0",
    "ink-spinner": "^5.0.0",
    "ink-text-input": "^6.0.0",
    "marked": "^15.0.12",
    "marked-terminal": "^7.3.0",
    "react": "^18.3.1"
  },
  "devDependencies": {
    "@types/marked-terminal": "^6.1.1",
    "@types/node": "^22.0.0",
    "@types/react": "^18.3.0",
    "@vitest/coverage-v8": "^4.0.15",
    "ink-testing-library": "^4.0.0",
    "tsx": "^4.19.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.15"
  }
}
